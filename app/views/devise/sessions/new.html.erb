<%= render '/posts/logo' %>
<div class="d_main">
  <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form-inputs">
      <div class="d_inputbox">
        <%= f.input :email, required: false, autofocus: true, label: '이메일' %>
        <%= f.input :password, required: false, label: '비밀번호' %>
      </div>
      <div class="d_button">
        <div class="form-actions login">
          <%= f.button :submit, "로그인", class:"btnlogin btnlogin-primary" %>
        </div>
      </div>
  <% end %>
  <%= render "devise/shared/links" %>
  </div>
    <!-- 뒤로가기 지워도 상관없음. -->
    <%= link_to '뒤로가기', :back, class: "d_link" %> 
</div>
<%= render '/posts/footer' %>